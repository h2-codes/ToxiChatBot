{
  "name": "toxichatbot",
  "version": "0.0.1",
  "description": "A Telegram group management bot",
  "main": "index",
  "module": "commonjs",
  "dependencies": {
    "@grammyjs/types": "^2.2.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^89.0.0",
    "grammy": "^1.3.3",
    "localenv": "^0.2.2",
    "mongoose": "^6.0.5",
    "ngrok": "^4.2.2",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.9.6",
    "@types/nodemon": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts,js,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/devStart.ts\"",
    "lint": "yarn run eslint src",
    "build": "tsc",
    "serve": "ts-node ./src/index.ts",
    "test": "jest",
    "test:docker": "docker-compose exec node sh -c \"./node_modules/.bin/jest --verbose\""
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@notabug.org:jimmyh/ToxiChatBot.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "javascript",
    "node"
  ],
  "author": "Jimmy Hogoboom",
  "license": "AGPL-3.0-or-later"
}
